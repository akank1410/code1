      int firstRepeated(int arr[], int n) {
        // code here
            
    int c;
    for(int i=0;i<n-1;i++)
    {
        if(find(arr+i+1,arr+n,arr[i]) !=&arr[n])
        return i+1;
    }
        
        return -1;
    }
    
    
