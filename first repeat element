  TC exceeds
  
  int firstRepeated(int arr[], int n) {
        // code here
unordered_map<int, int> m;
vector <int> v;
    
    for(int i=0; i<n;i++)
        { 
            m[arr[i]]++;
            if(m[arr[i]] > 1)
            {auto index = find(arr,arr+n,arr[i]);
                v.push_back(distance(arr,index)+1);}
        }
        sort(v.begin(),v.end());
        if(v.empty())
        return -1;
        else
        return v[0];
    }
    
