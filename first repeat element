        METHOD3: tc-> 0.59/2.54
        
        int firstRepeated(int arr[], int n) {
        // code here
        unordered_map<int,int> m ;
        for(int i=0;i<n;i++)
        m[arr[i]]++;
        
        for(int i=0;i<n;i++)
        if(m[arr[i]]>1)
        return i+1;
        
        return -1;
    }
    
