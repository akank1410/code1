#Move all negative elements to end 
 void segregateElements(int arr[],int n)
    {
        // Your code goes here
        int c=0;
        for(int i=n-1;i>=0;i--)
        {
            if(arr[i] < 0)
            {   c++;
                int temp = arr[i];
                for(int j =i; j<n-1;j++)
                arr[j] = arr[j+1];
                arr[n-1]=temp;
            }
        }
        reverse(arr+(n-c),arr+n);
    }
